{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "strict": true,
    "lib": [ "es2015", "dom" ],
    "types": [ "jquery", "bootstrap", "webpack-env" ],
    "typeRoots": [ "./node_modules/@types/" ]
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
